<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hzih.bsps.domain.ResourceIp" table="resourceip" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="resourceid" type="int">
            <column name="resourceid" />
        </property>
        <property name="ipaddress" type="java.lang.String">
            <column name="ipaddress" length="50"/>
        </property>
        <property name="subnetmask" type="java.lang.String">
            <column name="subnetmask" length="50"/>
        </property>
    </class>
</hibernate-mapping>