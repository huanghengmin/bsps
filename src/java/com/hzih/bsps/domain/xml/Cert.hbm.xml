<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hzih.bsps.domain.Cert" table="proxy_cert">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="key_path" type="java.lang.String">
            <column name="key_path" length="50"/>
        </property>
        <property name="cert_path" type="java.lang.String">
            <column name="cert_path" length="50"/>
        </property>
    </class>
</hibernate-mapping>